/**
 * And gate:
 * if (a and b) out = 1, else out = 0
 */
CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Nand(a=a, b=b, out=a-nand-b);
    Nand(a=a-nand-b, b=a-nand-b, out=out);
    
}